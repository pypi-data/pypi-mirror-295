{
  "version": 1,
  "formatters": {
    "verbose": {
      "format": "%(asctime)s | %(threadName)s | %(levelname)s | %(name)s.%(funcName)s | %(message)s",
      "datefmt": "%F %T"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "verbose"
    },
    "file": {
      "class": "logging.handlers.RotatingFileHandler",
      "filename": "workers.log",
      "maxBytes": 10485760,
      "backupCount": 10,
      "level": "INFO",
      "formatter": "verbose"
    }
  },
  "loggers": {
    "urllib3": {
      "level": "INFO"
    }
  },
  "root": {
    "level": "NOTSET",
    "handlers": [
      "console",
      "file"
    ]
  },
  "disable_existing_loggers": false
}
