{
  "name": "@ouyangde1/jupyterlab-spreadsheet",
  "version": "0.4.2",
  "description": "Adds a spreadsheet viewer to JupyterLab",
  "main": "lib/index.js",
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlab-spreadsheet/labextension"
  },
  "scripts": {
    "build": "tsc",
    "build:lab": "jupyter labextension build .",
    "install:lab": "jupyter labextension develop . --overwrite",
    "lint": "eslint --ext .ts src/**"
  },
  "keywords": [
    "jupyterlab-extension",
    "spreadsheet",
    "excel"
  ],
  "files": [
    "lib/*.js",
    "style/*.css",
    "jupyterlab-spreadsheet/**"
  ],
  "repository": "https://github.com/quigleyj97/jupyterlab-spreadsheet",
  "homepage": "https://github.com/quigleyj97/jupyterlab-spreadsheet",
  "author": {
    "name": "Joe Quigley"
  },
  "contributors": [
    {
      "name": "Mridul Seth",
      "email": "github@mriduls.com",
      "url": "https://mriduls.com"
    },
    {
      "name": "NQuigley02"
    }
  ],
  "license": "BSD-3-Clause",
  "private": false,
  "dependencies": {
    "@jupyterlab/application": "^4.0.0-rc.1",
    "@jupyterlab/apputils": "^4.0.0-rc.1",
    "@jupyterlab/docregistry": "^4.0.0-rc.1",
    "@lumino/coreutils": "^2.1.1",
    "@lumino/disposable": "^2.1.1",
    "@lumino/widgets": "^2.1.1",
    "expose-loader": "^3.0.0",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "rxjs": "^7.2.0",
    "slickgrid": "^2.3.21",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0-rc.1",
    "@types/slickgrid": "^2.1.27",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.30.0",
    "typescript": "^4.3.5"
  }
}
