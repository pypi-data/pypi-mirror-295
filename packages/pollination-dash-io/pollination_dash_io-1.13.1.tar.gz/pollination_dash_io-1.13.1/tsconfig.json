  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "src/ts",
    "inlineSources": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "target": "es5",
    "allowSyntheticDefaultImports": true,
    "importHelpers": true,
    "forceConsistentCasingInFileNames": true
